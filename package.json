{
  "name": "themeisle-site-onboarding",
  "version": "1.1.0",
  "description": "ThemeIsle Site Onboarding",
  "license": "GPL-3.0+",
  "author": "Themeisle <friends@themeisle.com> | Andrei Baicus <andrei@themeisle.com>",
  "scripts": {
    "phpcs": "phpcs --standard=phpcs.xml",
    "watchify": "watchify -vd -p browserify-hmr -e assets/vue/app.js -o assets/js/bundle.js",
    "dev": "npm-run-all --parallel watchify",
    "semantic-release": "semantic-release --debug",
    "build": "cross-env NODE_ENV=production browserify -g envify assets/vue/app.js | uglifyjs -c warnings=false -m > assets/js/bundle.js",
    "addToCommit": "git add assets/js"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.4.0",
    "@commitlint/config-conventional": "^7.3.1",
    "@semantic-release/changelog": "^3.0.1",
    "@semantic-release/exec": "^3.3.1",
    "@semantic-release/git": "^7.0.6",
    "babel-core": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.1",
    "browserify-hmr": "^0.3.1",
    "cross-env": "^1.0.6",
    "css-loader": "^0.23.0",
    "envify": "^3.4.1",
    "grunt-version": "^1.3.0",
    "http-server": "^0.11.1",
    "husky": "^1.3.1",
    "load-project-config": "~0.2.0",
    "node-sass": "^4.10.0",
    "npm-run-all": "^2.1.2",
    "pre-commit": "^1.2.2",
    "sass-loader": "^7.1.0",
    "semantic-release": "^15.13.1",
    "style-loader": "^0.13.0",
    "uglify-js": "^2.5.0",
    "vue-style-loader": "^4.1.1",
    "vueify": "^9.1.0",
    "watchify": "^3.4.0"
  },
  "dependencies": {
    "v-clipboard": "^2.2.1",
    "vue": "^2.4.2",
    "vue-clickaway": "^2.1.0",
    "vue-js-toggle-button": "^1.2.3",
    "vue-moment": "^3.2.0",
    "vue-resource": "^1.3.4",
    "vuex": "^2.4.0"
  },
  "browserify": {
    "transform": [
      "vueify",
      "babelify"
    ]
  },
  "browser": {
    "vue": "vue/dist/vue.common.js"
  },
  "pre-commit": {
    "run": [
      "phpcs",
      "build",
      "addToCommit"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
